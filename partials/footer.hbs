<script>
  // Google analytics
  window.dataLayer = window.dataLayer || [];
  function gtag() { dataLayer.push(arguments); }
  gtag('js', new Date());

  gtag('config', 'UA-162746878-1');

  // A hacky way to give each heading an anchor link.
  try {
    var h2s = Array.from(document.querySelectorAll('.article h2'))
    var h3s = Array.from(document.querySelectorAll('.article h3'))
    var headings = [].concat(h2s, h3s)
    if (headings && headings.length) {
      headings.map(function (h) {
        var link = document.createElement('a')
        link.href = '#' + h.id
        link.className = 'anchor'
        link.innerHTML = '#'
        h.appendChild(link)
      })
    }
  } catch error {
    console.error('CANNOT SET ANCHOR LINKS:')
    console.error(error)
  }
</script>
